<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigator"
    app:startDestination="@id/fragment_info">
    <fragment
        android:id="@+id/fragment_info"
        android:name="ru.svolf.anonfiles.presentation.info.InfoFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_info">
        <action
            android:id="@+id/action_toSettings"
            app:destination="@id/settings_fragment" />
    </fragment>
    <fragment
        android:label="@string/title_settings"
        android:id="@+id/settings_fragment"
        android:name="ru.svolf.anonfiles.presentation.settings.SettingsFragment"/>
</navigation>